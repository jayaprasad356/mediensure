<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OverViewActivity"
    android:background="#EAEAEA"
    >

    <ImageButton
        android:id="@+id/ibBack"
        android:layout_width="@dimen/_30sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_margin="@dimen/_10sdp"
        android:background="@drawable/ic_baseline_chevron_left_24"
        />
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="@color/black"
            android:layout_marginTop="20dp"
            android:textStyle="bold"
            android:text="Home"
            android:textSize="20dp"
            android:padding="@dimen/_10sdp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:layout_margin="10dp"
            android:textStyle="bold"
            android:text="Overview"
            android:textSize="20dp"
            android:padding="@dimen/_10sdp"
            />
        
        
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="@dimen/_10sdp"
            android:layout_marginHorizontal="@dimen/_10sdp"
            >
            
            <com.google.android.material.card.MaterialCardView
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_120sdp"
                app:cardCornerRadius="@dimen/_10sdp"
                android:elevation="@dimen/_2sdp"
                >



                <LinearLayout
                    android:id="@+id/llOpdNetwork"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:padding="@dimen/_10sdp">

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        app:cardCornerRadius="@dimen/_60sdp"
                        android:backgroundTint="@color/colorPrimarylight"
                        >



                        <ImageView
                            android:layout_gravity="center"
                            android:layout_width="@dimen/_18sdp"
                            android:layout_height="@dimen/_18sdp"
                            android:background="@drawable/ic_heart"
                            android:backgroundTint="@color/colorPrimary"
                            android:padding="@dimen/_8sdp"
                           />

                    </com.google.android.material.card.MaterialCardView>


                    <TextView
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_width="match_parent"
                        android:textAlignment="center"
                        android:layout_height="wrap_content"
                        android:text="OPD\nNetwork"
                        android:textStyle="bold"
                         android:textSize="@dimen/_10sdp"
                        android:textColor="@color/black"/>



                </LinearLayout>




            </com.google.android.material.card.MaterialCardView>
            <Space
                android:layout_width="@dimen/_10sdp"
                android:layout_height="wrap_content"/>
            <com.google.android.material.card.MaterialCardView
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_120sdp"
                app:cardCornerRadius="@dimen/_10sdp"
                android:elevation="@dimen/_2sdp"
                >



                <LinearLayout
                    android:id="@+id/llLabNetwork"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:padding="@dimen/_10sdp">


                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        app:cardCornerRadius="@dimen/_60sdp"
                        android:backgroundTint="@color/colorPrimarylight"
                        >



                    <ImageView
                        android:layout_gravity="center"
                        android:layout_width="@dimen/_18sdp"
                        android:layout_height="@dimen/_18sdp"
                        android:background="@drawable/ic_flask"
                        android:backgroundTint="@color/colorPrimary"
                        android:padding="@dimen/_8sdp"
                        android:src="@drawable/ic_profile" />

                    </com.google.android.material.card.MaterialCardView>

                    <TextView
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_width="match_parent"
                        android:textAlignment="center"
                        android:layout_height="wrap_content"
                        android:text="Lab \nNetwork"
                        android:textStyle="bold"
                         android:textSize="@dimen/_10sdp"
                        android:textColor="@color/black"/>


                </LinearLayout>




            </com.google.android.material.card.MaterialCardView>
            <Space
                android:layout_width="@dimen/_10sdp"
                android:layout_height="wrap_content"/>

            <com.google.android.material.card.MaterialCardView
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_120sdp"
                app:cardCornerRadius="@dimen/_10sdp"
                android:elevation="@dimen/_2sdp"
                >



                <LinearLayout
                    android:id="@+id/llPharmacyNetwork"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:padding="@dimen/_10sdp">

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        app:cardCornerRadius="@dimen/_60sdp"
                        android:backgroundTint="@color/colorPrimarylight"
                        >



                        <ImageView
                            android:layout_gravity="center"
                            android:layout_width="@dimen/_18sdp"
                            android:layout_height="@dimen/_18sdp"
                            android:backgroundTint="@color/colorPrimary"
                            android:background="@drawable/ic_pharmacy"
                            android:padding="@dimen/_8sdp"
                             />

                    </com.google.android.material.card.MaterialCardView>

                    <TextView
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_width="match_parent"
                        android:textAlignment="center"
                        android:layout_height="wrap_content"
                        android:text="Pharmacy\nNetwork"
                        android:textStyle="bold"
                         android:textSize="@dimen/_10sdp"
                        android:textColor="@color/black"/>



                </LinearLayout>




            </com.google.android.material.card.MaterialCardView>
            
            
            
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="@dimen/_10sdp"
            android:layout_marginHorizontal="@dimen/_12sdp"


            >

            <com.google.android.material.card.MaterialCardView
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_120sdp"
                app:cardCornerRadius="@dimen/_10sdp"
                android:elevation="@dimen/_2sdp"
                android:padding="@dimen/_10sdp"
                >



                <LinearLayout
                    android:id="@+id/llDentalNetwork"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:padding="@dimen/_10sdp">

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        app:cardCornerRadius="@dimen/_60sdp"
                        android:backgroundTint="@color/colorPrimarylight"
                        >


                        <ImageView
                            android:layout_width="@dimen/_18sdp"
                            android:layout_height="21dp"
                            android:layout_gravity="center"
                            android:background="@drawable/ic_teeth"
                            android:backgroundTint="@color/colorPrimary"
                            android:padding="@dimen/_8sdp"
                            />

                    </com.google.android.material.card.MaterialCardView>

                    <TextView
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_width="match_parent"
                        android:textAlignment="center"
                        android:layout_height="wrap_content"
                        android:text="Dental\nProvider\nNetwork"
                        android:textStyle="bold"
                         android:textSize="@dimen/_10sdp"
                        android:textColor="@color/black"/>



                </LinearLayout>




            </com.google.android.material.card.MaterialCardView>
            <Space
                android:layout_width="@dimen/_10sdp"
                android:layout_height="wrap_content"/>
            <com.google.android.material.card.MaterialCardView
                 android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_120sdp"
                app:cardCornerRadius="@dimen/_10sdp"
                android:elevation="@dimen/_2sdp"
                >



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:padding="@dimen/_10sdp">

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        app:cardCornerRadius="@dimen/_60sdp"
                        android:backgroundTint="@color/colorPrimarylight"
                        >



                        <ImageView
                            android:layout_gravity="center"
                            android:layout_width="@dimen/_18sdp"
                            android:layout_height="@dimen/_18sdp"
                            android:background="@drawable/ic_flask"
                            android:padding="@dimen/_8sdp"
                            android:src="@drawable/ic_profile" />

                    </com.google.android.material.card.MaterialCardView>

                    <TextView
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_width="match_parent"
                        android:textAlignment="center"
                        android:layout_height="wrap_content"
                        android:text="Radiology Network "
                        android:textStyle="bold"
                         android:textSize="@dimen/_10sdp"
                        android:textColor="@color/black"/>



                </LinearLayout>




            </com.google.android.material.card.MaterialCardView>
            <Space
                android:layout_width="@dimen/_10sdp"
                android:layout_height="wrap_content"/>

            <com.google.android.material.card.MaterialCardView
                android:visibility="invisible"
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_120sdp"
                app:cardCornerRadius="@dimen/_20sdp"
                android:elevation="@dimen/_2sdp"
                >



                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:padding="@dimen/_10sdp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_profile"
                        android:background="@drawable/bluecirclebackground"
                        android:padding="@dimen/_8sdp"
                        />

                    <TextView
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="OPD"
                        android:textStyle="bold"
                        android:layout_gravity="start"
                        android:textSize="@dimen/_10sdp"
                        android:textColor="@color/black"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Network"
                        android:layout_gravity="start"
                        android:textSize="@dimen/_12sdp"
                        android:textColor="@color/black"/>

                </LinearLayout>




            </com.google.android.material.card.MaterialCardView>



        </LinearLayout>

        
    </LinearLayout>

</RelativeLayout>